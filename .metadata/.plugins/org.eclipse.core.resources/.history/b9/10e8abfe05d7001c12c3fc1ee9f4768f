package com.eduardo.cursojava.aula2;

public class VariareisPontoFlutuante {

	public static void main(String[] args) {
		
		double valorPassagem = 2.90;
		
		float valorTomate = 3.95f;
		
		System.out.println("Qual valor da passagem = " + valorPassagem);
		System.out.println("Qual valor do tomate = " + valorTomate);

	}

}
